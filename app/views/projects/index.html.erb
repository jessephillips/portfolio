<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Image</th>
      <th>Short desc</th>
      <th>Long desc</th>
      <th>Project link</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.image %></td>
        <td><%= project.short_desc %></td>
        <td><%= project.long_desc %></td>
        <td><%= project.project_link %></td>
        <td><%= link_to 'Show', project %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
